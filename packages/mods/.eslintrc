{
  "env": {
    "jest": true
  },
  "extends": ["eslint:recommended"],
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "script"
  },
  "rules": {
    "camelcase": "off",
    "no-else-return": "off",
    "arrow-body-style": "off",
    "import/prefer-default-export": "off",
    "class-methods-use-this": "off",
    "flowtype/no-types-missing-file-annotation": 2,
    "no-underscore-dangle": ["error", { "allowAfterThis": true }]
  }
}
