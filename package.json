{
  "name": "recraft",
  "description": "dynamic craftable mods based on recent data mining",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "repository": {
    "url": "https://github.com/eps1lon/poe-recraft.git",
    "type": "git"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-flow-runtime": "^0.11.1",
    "cross-env": "^5.0.5",
    "fetch-mock": "^5.12.2",
    "flow-bin": "^0.53.1",
    "flow-runtime": "^0.14.0",
    "flow-typed": "^2.1.5",
    "husky": "^0.14.3",
    "prettier": "^1.6.1",
    "redux-mock-store": "^1.2.3"
  },
  "dependencies": {
    "bootstrap": "^4.0.0-alpha.6",
    "lodash": "^4.17.4",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.1.1",
    "poe-mods": "^1.4.0",
    "react": "16.0.0-beta.5",
    "react-addons-css-transition-group": "^15.6.2",
    "react-addons-transition-group": "^15.6.2",
    "react-dom": "16.0.0-beta.5",
    "react-redux": "^5.0.6",
    "react-scripts": "1.0.12",
    "react-table": "^6.5.3",
    "reactstrap": "^4.8.0",
    "reduce-reducers": "^0.1.2",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-api-middleware": "^2.0.0-beta.3",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.15.6",
    "reselect": "^3.0.1"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./node_modules src/ -o src/",
    "build": "npm run build-css && react-scripts build",
    "eject": "react-scripts eject",
    "flow": "flow",
    "format": "prettier --write src/**/*.js",
    "prepush": "prettier --list-different src/**/*.js && cross-env CI=true yarn test",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "test": "react-scripts test --env=jsdom",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./node_modules src/ -o src/ --watch"
  }
}
