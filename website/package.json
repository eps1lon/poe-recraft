{
  "name": "website",
  "description": "dynamic craftable mods based on recent data mining",
  "homepage": ".",
  "version": "0.2.0",
  "private": true,
  "license": "MIT",
  "repository": {
    "url": "https://github.com/eps1lon/poe-recraft.git",
    "type": "git"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.3",
    "@types/fetch-mock": "^7.2.1",
    "@types/flat": "0.0.28",
    "@types/jest": "^23.0.2",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.5",
    "@types/react-intl": "^2.3.5",
    "@types/react-redux": "^5.0.15",
    "@types/react-transition-group": "^2.0.7",
    "@types/reactstrap": "^5.0.19",
    "@types/redux-logger": "^3.0.5",
    "@types/redux-mock-store": "^1.0.0",
    "@types/webpack-env": "^1.13.6",
    "cross-env": "^5.0.5",
    "fetch-mock": "^5.12.2",
    "gh-pages": "^1.0.0",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.1",
    "react-scripts": "^3.2.0",
    "react-test-renderer": "^16.0.0",
    "recraft-scripts": "*",
    "redux-mock-store": "^1.2.3",
    "rimraf": "^2.6.2",
    "tslint-config-prettier": "^1.9.0",
    "typescript": "~3.2.2"
  },
  "dependencies": {
    "bootstrap": "^4.0.0-alpha.6",
    "bootswatch": "^4.0.0",
    "classnames": "^2.2.5",
    "flat": "^4.0.0",
    "lodash": "^4.17.4",
    "poe-components-item": "^0.4.1",
    "poe-i18n": "^0.12.0",
    "poe-mods": "^1.12.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-intl": "^2.4.0",
    "react-redux": "^5.0.7",
    "react-transition-group": "^2.3.0",
    "reactstrap": "^5.0.0-beta.2",
    "redux": "^4.0.0-beta.1",
    "redux-api-middleware": "^2.0.0-beta.3",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.15.6",
    "reselect": "^3.0.1"
  },
  "scripts": {
    "build": "npm run build:css && npm run build:js",
    "build:css": "node-sass-chokidar --include-path ../node_modules src/ -o src/",
    "build:js": "react-scripts build",
    "build:clean": "rimraf build",
    "clean": "yarn build:clean",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "eject": "react-scripts eject",
    "format": "recraft-format \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json \"src/**/*.{ts,tsx}\"",
    "prepush": "prettier --list-different \"--write src/**/*.ts\" && cross-env CI=true yarn test",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "test": "react-scripts test --env=jsdom",
    "watch-css": "npm run build:css && node-sass-chokidar --include-path ../node_modules src/ -o src/ --watch"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
