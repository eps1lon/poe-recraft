{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "rules": {
    "object-literal-sort-keys": false,
    "interface-name": [true, "never-prefix"],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-pascal-case",
      "allow-snake-case"
    ],
    "jsx-no-lambda": false,
    "no-empty": false,
    "no-console": false,
    "member-ordering": [
      true,
      "public-before-private",
      "static-before-instance",
      "variables-before-functions"
    ]
  }
}
